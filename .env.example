# NewStore Configuration
# Copy this file to .env and fill in your actual values

# Required: Authentication token for the API service
TAP_NEWSTORE_AUTH_TOKEN=your_auth_token_here

# Required: Project IDs to replicate (comma-separated)
TAP_NEWSTORE_PROJECT_IDS=project1,project2,project3

# Optional: The earliest record date to sync (ISO format)
TAP_NEWSTORE_START_DATE=2023-01-01T00:00:00Z

# Optional: API URL (defaults to https://api.mysample.com)
TAP_NEWSTORE_API_URL=https://api.mysample.com

# Optional: Custom User-Agent header
TAP_NEWSTORE_USER_AGENT=tap-newstore/1.0.0
